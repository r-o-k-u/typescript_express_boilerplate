{
  "name": "typescript_boilerplate",
  "version": "1.0.0",
  "description": "Qwerty systems typescript api boilerplate",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --config nodemon.json bin/www.ts",
    "start": "nodemon --config nodemon.json bin/www.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "generate-doc": "npx typedoc",
    "make:migration": "node ./node_modules/sequelize-auto-migrations/bin/makemigration --name test",
    "db:init": "npx sequelize init ",
    "db:init:migrations": "npx sequelize init ",
    "db:init:seeders": "npx sequelize init ",
    "db:create": "npx sequelize db:create ",
    "db:migrate": "npx sequelize db:migrate",
    "db:migration:generate": "npx sequelize migration:generate --name test",
    "db:seeder:generate": "npx sequelize seed:generate --name test",
    "db:migrations:add:timestamp": " sequelize db:migrate:schema:timestamps:add",
    "db:migrateundo": "npx sequelize db:migrate:undo:all ",
    "db:seed": "npx sequelize db:seed:all ",
    "db:seedundo": "npx sequelize db:seed:undo:all",
    "db:drop": "npx sequelize db:drop",
    "db:model:create": "npx sequelize model:generate"
  },
  "keywords": [
    "typescript",
    "api",
    "node",
    "js",
    "express",
    "boilerplate"
  ],
  "author": "r-o-k-u <Corneliusokaya@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.4",
    "bcrypt": "^5.1.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-ejs-layouts": "^2.5.1",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.20",
    "npm": "^7.6.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.26.0",
    "sequelize-cli": "^6.5.2",
    "sequelize-typescript": "^2.1.5",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "0.0.41",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.18.7",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.10",
    "@types/yup": "^0.29.14",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.5.2"
  }
}
